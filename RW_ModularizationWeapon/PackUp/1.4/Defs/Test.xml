				<TextureFilterMode>Bilinear</TextureFilterMode>
				<ForceNodeIdControl>true</ForceNodeIdControl>
			</li>
            <li Class="RW_ModularizationWeapon.CompProperties_ModularizationWeapon">
				<verbPropertiesMultiplier>
					<ticksBetweenBurstShots>0.25</ticksBetweenBurstShots>
					<warmupTime>0.25</warmupTime>
				</verbPropertiesMultiplier>
				<verbPropertiesAffectByOtherPart>true</verbPropertiesAffectByOtherPart>
                <attachmentProperties>
                    <li>
						<id>test-1</id>
						<postion>(1,0,-1)</postion>
						<defultThing>Gun_ChainShotgun</defultThing>
						<allowEmpty>true</allowEmpty>
                        <filter>
                            <thingDefs>
                                <li>Gun_ChainShotgun</li>
                            </thingDefs>
                        </filter>
                    </li>
                    <li>
						<id>test-2</id>
						<postion>(0,0,1)</postion>
						<defultThing>Gun_HeavySMG</defultThing>
						<allowEmpty>true</allowEmpty>
                        <filter>
                            <thingDefs>
